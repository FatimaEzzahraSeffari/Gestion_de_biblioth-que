@{
    Layout = null;
}
@model GestionBibliotheque_TP.Models.UserRegister


<!-- Customized Bootstrap Stylesheet -->
<link href="~/css/bootstrap.min.css" rel="stylesheet">

<!-- Template Stylesheet -->
<link href="~/css/style.css" rel="stylesheet">


<div class="card border-danger custom-card-style">
    <div class="card-body">
        <h1 class="mb-0 text-primary text-uppercase"><img src="/img/logo3.png" alt="Logo" class="me-3" height="100" width="90">BiblioMate</h1>
        <div class="row no-gutters">
            <!-- First Column: Form -->
            <div class="col-md-10">
                <h2 class="card-title text-center" style="color:red;">Créer Un Compte</h2>
                <form id="registerForm" asp-action="Register" method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <br />
            <div class="form-group row">
                        <label asp-for="FirstName" class="col-md-2 col-form-label black-text text-md-right" style="font-weight: bold;font-size:18px;">Prénom :</label>
                <div class="col-md-10">
                    <input asp-for="FirstName" class="form-control form-control-sm red-border white-text" />
                    <span asp-validation-for="FirstName" class="text-danger"></span>
                </div>
            </div>
            <br />
            <div class="form-group row">
                        <label asp-for="LastName" class="col-md-2 col-form-label black-text text-md-right" style="font-weight: bold;font-size:18px;">Nom:</label>
                <div class="col-md-10">
                    <input asp-for="LastName" class="form-control form-control-sm red-border white-text" />
                    <span asp-validation-for="LastName" class="text-danger"></span>
                </div>
            </div>
            <br />
            <div class="form-group row">
                        <label asp-for="Email" class="col-md-2 col-form-label black-text" style="font-weight: bold;font-size:18px;">Email:</label>
                <div class="col-md-10">
                    <input asp-for="Email" class="form-control red-border white-text" />
                    <span asp-validation-for="Email" class="text-danger"></span>
                </div>
            </div>
            <br />
            <div class="form-group row">
                        <label asp-for="Password" class="col-md-2 col-form-label black-text" style="font-weight: bold;font-size:18px;">Mot de passe :</label>
                <div class="col-md-10">
                    <input asp-for="Password" type="password" class="form-control red-border white-text" />
                    <span asp-validation-for="Password" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group row">
                <div class="offset-md-2 col-md-10">
                            <center> <button type="submit" class="btn btn-primary" style="width: 130px;border-color: white;font-weight: bold;color:black;">Register</button></center>
                           

                </div>
            </div>

                </form>
            </div>

            <!-- Second Column: Image -->
          
        </div>
    </div>
</div>

<style>
    body {
        background-color: #222;
        background: url('data:image/jpeg;base64,') center/cover no-repeat;
        height: 100vh;
        overflow: hidden;
        color: #FFF;
        font-family: Roboto;
        position: relative;
    }

        body::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(71, 71, 71, 0.1); /* Adjust the color and opacity as needed */
        }


    

   /*  .container {
        margin-top: 100px; /* Adjust the margin-top to center the form vertically */
    } */

    .page-title {
        text-align: center;
        font-weight: bold;
        margin-bottom: 40px;
        color: white;
    }

    .red-border {
        border: 2px solid red;
    }

    .white-text {
        color: white;
    }

    .custom-card-style {
        width: 60% !important;
        background-color: rgba(71, 71, 71, 0.8);
        !important;
        margin: auto !important;
        margin-bottom: 50px !important;
    }

   

    .card.border-danger.custom-card-style {
        width: 60% !important;
        height: 97% !important;
        background: rgba(71, 71, 71, 0.8); /* Adjust the color and opacity as needed */
        margin: auto;
        margin-bottom: 50px;
        padding: 25px;
        position: relative;
        top: 2%; /* Adjust the top value as needed */
    }


    .form-group {
        margin-bottom: 1rem;
    }

    label {
        font: 500 14px Roboto;
        color: black;
        
    }

    input {
        background: rgba(#FFF, .15);
        width: 100%;
        height:40px;
        padding: 8px;
        margin: 5px 0;
        border: none;
        border-radius: 3px;
        outline: none;
        color: #FFF;
        font-size: 20px;
    }

    .btn-primary {
        background: linear-gradient( -45deg, red, red);
        padding: 10px 20px;
        border: none;
        border-radius: 21px;
        outline: none;
        overflow: hidden;
        position: relative;
        bottom: 10px;
        color: #FFF;
        font-size: 18px;
        cursor: pointer;
        &:hover:before

    {
        left: 110%;
        transition: .3s;
    }

    &:before {
        content: '';
        background: rgba(#FFF, .3);
        height: 100%;
        width: 65px;
        position: absolute;
        top: 0;
        left: -100%;
        transform: skew(-45deg);
        transition: 0s;
    }

    
</style>
